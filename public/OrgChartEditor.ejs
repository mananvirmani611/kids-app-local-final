	<script src="./OrgChart/MindFusion.Common.js" type="text/javascript"></script>
	<script src="./OrgChart/MindFusion.Diagramming.js" type="text/javascript"></script>
	<script src="./check/jQuery-Plugin-For-Checkable-Images-imgCheckbox/jquery.imgcheckbox.js"></script>
	<script src="./OrgChart/OrgChartEditor.js" type="text/javascript"></script>
	<script src="./OrgChart/main.js"></script>
	<script>
		function setyourimg(url) {
			for (const key in diagram.items) {
				if (Object.hasOwnProperty.call(diagram.items, key)) {
					const element = diagram.items[key];
					if (element.selected) {
						diagram.items[key].setImageLocation(url);
						let ph = document.getElementById("diagram_inplaceInput")
					    ph.innerText = url;
					}
				}
			}
		};

	</script>
